{
   "visitCountAll": {
       "name": "Visit Count Report",
       "type": "aggregation",
       "config": {
           "report":{
               "type": "visits",
                "config": {
                    "applyDateRangeFor": "visitStartDate"
                }
           },
           "rowGroups": [
               "Visit Type"
           ],
           "distinctGroups": [
               "Patient Identifier"
           ],
            "showTotalRow":true
       }
   },
   "visitCountNB": {
    "name": "Visit Count Report for Non Bhutanese",
    "type": "MRSGeneric",
    "config": {
      "sqlPath": "/var/www/bahmni_config/openmrs/apps/reports/SQL/visitCountNB.sql"
    }
  },
  "visitCountGender": {
         "name": "Visit Count Report Based on Gender",
         "type": "aggregation",
         "config": {
             "report":{
                 "type": "visits",
                  "config": {
                      "applyDateRangeFor": "visitStartDate",
                      "patientAttributes": ["Gender"]
                  }
             },
             "rowGroups": [
                 "Visit Type"
             ],
              "columnGroups": [
                 "Gender"
             ],
             "distinctGroups": [
                 "Patient Identifier"
             ],
              "showTotalRow":true,
              "showTotalColumn":true

         }
     },
  "morbidityReport": {
            "name": "Morbidity Report",
            "type": "diagnosisCount",
            "config": {
              "dateRangeRequired": true,
              "applyDateRangeFor":"diagnosisDate",
              "concept": "Diagnosis Set of Sets",
              "ageGroupName": "Diagnosis Age Group",
              "rowsGroupBy": ["header_concept_name","leaf_concept_name"],
             "columnsGroupBy": ["gender","agegroup_name"]
             }
   }
}
