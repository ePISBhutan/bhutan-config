{
   "Visitcount_All": {
       "name": "Visit Count Report",
       "type": "aggregation",
       "config": {
           "report":{
               "type": "visits",
                "config": {
                    "applyDateRangeFor": "visitStartDate"
                }
           },
           "rowGroups": [
               "Visit Type"
           ],
           "distinctGroups": [
               "Patient Identifier"
           ],
            "showTotalRow":true
       }
   },

  "Visitcount_NB": {
         "name": "Visit Count Report for Non Bhutanese",
         "type": "aggregation",
         "config": {
             "report":{
                 "type": "visits",
                  "config": {
                      "applyDateRangeFor": "visitStartDate",
                      "patientAttributes": ["NonResidentialBhutaneseType"]
                  }
             },
             "rowGroups": [
                 "Visit Type"
             ],
              "columnGroups": [
                 "NonResidentialBhutaneseType"
             ],
             "distinctGroups": [
                 "Patient Identifier"
             ],
              "showTotalRow":true,
              "showTotalColumn":true

         }
     },

  "Visitcount_Gender": {
         "name": "Visit Count Report Based on Gender",
         "type": "aggregation",
         "config": {
             "report":{
                 "type": "visits",
                  "config": {
                      "applyDateRangeFor": "visitStartDate",
                      "patientAttributes": ["Gender"]
                  }
             },
             "rowGroups": [
                 "Visit Type"
             ],
              "columnGroups": [
                 "Gender"
             ],
             "distinctGroups": [
                 "Patient Identifier"
             ],
              "showTotalRow":true,
              "showTotalColumn":true

         }
     }
}
